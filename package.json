    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "prettier": "2.0.5",
    "react-test-renderer": "^16.13.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-graphql": "0.0.6",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.3"
  },
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "liontechnyc"
  ],
  "license": "GPL-2.0-only",
  "scripts": {
    "build": "npm run optimize && gatsby build",
    "develop": "npm run optimize && gatsby develop -p 5000",
    "format": "tslint \"{src}/**/*.{ts,tsx}\" --quiet --fix || prettier --write \"*/**/*.{js,json,md,scss}\"",
    "lint-staged": "tslint \"src/**/*.{js,ts,tsx}\" --fix || prettier --write \"*/**/*.{js,json,md,scss}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "stage": "npm run build && npm run serve",
    "clean": "gatsby clean",
    "test": "jest",
    "optimize": "ts-node scripts/optimize.ts",
    "type-check": "tsc --noEmit",
    "preinstall": "git install lfs || rm -rf .git/hooks"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/liontechnyc/frontend/liontech.nyc.git"
  },
  "bugs": {
    "url": "https://liontech.nyc/#footer"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "post-checkout": "echo $HUSKY_GIT_STDIN | lfs-hooks/post-checkout $HUSKY_GIT_PARAMS",
      "post-commit": "echo $HUSKY_GIT_STDIN | lfs-hooks/post-commit $HUSKY_GIT_PARAMS",
      "post-merge": "echo $HUSKY_GIT_STDIN | lfs-hooks/post-merge $HUSKY_GIT_PARAMS",
      "pre-push": "echo $HUSKY_GIT_STDIN | lfs-hooks/pre-push $HUSKY_GIT_PARAMS"
    }
  }
}
